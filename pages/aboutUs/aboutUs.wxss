.aboutUsTitle{
  
   font-size: 18px;
   text-align: center;
   width: 100%;
   
}
.aboutUsBg{
    width: 100%;
    height:350rpx;
}
.aboutUsMsg{
    width: 100%;
    height:350rpx;
    position: absolute;
    top:0;
    left:0;
    line-height: 350rpx;
    color:#fff;
    font-size:16px;
    background: rgba(0,0,0,0.2);
    font-weight: 500
}
.aboutUsContentTitle{
    font-size:16px;
    width:95%;
    height:100rpx;
    line-height: 100rpx;
    font-weight: 600;
    color:#A9A9A9;
    margin:auto;

}
.aboutUsContent{
    width:95%;
    margin:auto;
    font-size: 12px;
    color:#5e7387;
    text-indent: 2em;
    line-height: 60rpx;
}
.contactUs{
    width:100%;
    margin-bottom: 25px;
}
.contactUsInput{
    width:90%;
    margin: auto;
    height:80rpx;
    line-height: 80rpx;
    border:1px solid #eee;
    border-radius: 10px;
    padding-left:20rpx;
    margin-bottom: 25rpx;
    font-size: 14px;
}
.contactUsBtn{
    width:90%;
    background:rgba(0,0,0,0.6);
    color:#fff;
    font-size: 14px;
}
.phoneWarn{
    color:red;
    font-size:12px;
    width:90%;
    text-align: left;
    display: block;
    margin:auto
}